{
    "compilerOptions": {
      "lib": [
        "es5",
        "es6"
      ],
        "outDir": "./dist",
        "module": "CommonJS",
        "target": "ES6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowJs": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
    }
}